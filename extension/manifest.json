{

  "manifest_version": 2,
  "name": "Open Last Download",
  "description": "This extension opens the last available download.",
  "version": "0.1",
  "permissions": ["storage","tabs","downloads", "downloads.open"],
  "background": {
    "scripts": ["assets/js/background.js"]
  },
  "icons": {
    "128": "./assets/images/icon.png"
  },
  "commands": {
    "open-last-download": {
      "suggested_key": {
        "default": "Alt+X"
      },
      "description": "Open Last Download"
    },

    "open-file-explorer": {
      "suggested_key": {
        "default": "Alt+Y"
      },
      "description": "Open File Explorer"
    },

    "show-last-download": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "Show Downloaded Files"
    }


  }

}


